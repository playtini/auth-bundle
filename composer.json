{
    "name": "playtini/auth-bundle",
    "description": "Simple Google Account Authentication Symfony Bundle",
    "type": "symfony-bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Andrew Shukailo",
            "email": "ash@playtini.ua"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Playtini\\Bundle\\AuthBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "Tests/"
        }
    },
    "require": {
        "php": ">=7.0",
        "knpuniversity/oauth2-client-bundle": "^1.1",
        "league/oauth2-google": "^1.0",
        "twig/twig": "^1.24",
        "symfony/security-guard": "^3.0",
        "doctrine/doctrine-bundle": "^1.6"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.0",
        "phpunit/phpunit": "^5.6"
    },
    "config": {
        "platform": {
            "php": "7.0"
        }
    }
}
